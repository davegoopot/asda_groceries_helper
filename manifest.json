{
    "name": "UK Online Groceries Helper",
    "manifest_version": 2,
    "version": "1.0",
    "description": "This extension helps with filling a shopping basket on https://groceries.asda.com/ and https://groceries.morrisons.com/ .",
    "homepage_url": "https://github.com/davegoopot/asda_groceries_helper",
    "content_scripts": [
        {
            "matches": [
                "https://groceries.asda.com/*",
                "https://groceries.morrisons.com/*"
            ],
            "js": [
                "shops.js"
            ]
        }
    ]
}